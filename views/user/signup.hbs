<div class="row">
  <div class="col-md-4 col-md-offset-4">
      <h1>SignUp</h1>
      {{#each mess}}
        {{ this.msg }}
          
      {{/each}}
      <form method="POST" autocomplete="off">
        <input type='hidden' name='_csrf' value="{{ csrfToken }}"/>
        <input type='text' name='username' placeholder='Username' value="{{username}}"/><br><br>
        <input type='text' name='email' placeholder='Email' value="{{email}}"/><br><br>
        <input type='password' name='password' placeholder='Password' value="{{password}}"/><br><br>
        <input type='password' name='password2' placeholder='Confirm Password' /><br><br>
        <button type='submit' class="btn btn-primary pull-right" role="button" >Submit</button>
      </form>
      {{ csrfToken}}
  </div>
</div>